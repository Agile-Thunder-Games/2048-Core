{
	"compilerOptions": {
		"target": "es5",
		"module": "system",
        "noImplicitThis": true,
        "strictNullChecks": false,
		"moduleResolution": "node",
		"sourceMap": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"removeComments": true,
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true,
        "diagnostics": true,
        "rootDir": "src",
		"outDir": "dist/bin"
	},
	"exclude": [
        "node_modules"
    ]
}